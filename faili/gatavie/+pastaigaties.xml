	<SENTENCES>
		<SENTENCE CATEGORY="blank">
			<TEXT>Piemēram , ja <WORD json="['Acc','Nom','Gen','Dat','Loc']" attribute="Nom" class="cFFFF00">jūs</WORD> , <WORD json="['V1','V2','V3','Inf','Part']" attribute="Part" class="verbs">pastaigājoties</WORD>  <WORD json="['ArDat','ArAcc']" attribute="ArAcc" class="c0000FF">ar mīļoto</WORD> , sajūtat iekšēju šķērsli</TEXT>
			<COMMENT>Gunta. Gatavs</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['Adv']" attribute="Adv" class="c868A08">Tovakar</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">viņi</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD>  un skūpstījās naksnīgajās ielās,</TEXT>
			<COMMENT>Nom V3 Loc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">Dakteris</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD>  <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c00FF00">pa galeriju</WORD> un pierakstīja bloknotā,</TEXT>
			<COMMENT>Nom V3 paAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c088A08">Pa asfaltu</WORD>nopucētās kurpēs <WORD json="['Acc','Nom','Gen','Dat','Loc','Adv']" attribute="Adv" class="c00FF80">lēni</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V2" class="verbs">pastaigājās</WORD>  <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">vīrieši un sievietes</WORD> — bija redzamas</TEXT>
			<COMMENT>paAcc V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>tādēļ treniņi notika ārā . <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">Gastons</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD>  <WORD json="['GarDat','GarAcc']" attribute="GarAcc" class="c088A08">gar žogu</WORD> un sākumā neuzkrītoši skatījās</TEXT>
			<COMMENT>Nom V3 garAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>uz nometni . <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">Viņš</WORD> iegāja parkā , <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD>, tad apsēdās uz soliņa . No skolas</TEXT>
			<COMMENT>Nom V3</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>ārā pa logu virsū cilvēkam , <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">kurš</WORD> <WORD json="['Adv']" attribute="Adv" class="c00FF00">šeit</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD>  kā pa sev svešas realitātes muzeju .</TEXT>
			<COMMENT>Nom Adv V3</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>augļa ar nenosakāmu gadu skaitu . <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">Es</WORD> <WORD json="['V1','V2','V3','Inf']" attribute="V3" class="verbs">pastaigājos</WORD>  <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c00FF00">pa savu vecumdienu dzīves <strong>rajonu</strong></WORD> .</TEXT>
			<COMMENT>Nom V3 paAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">kungs</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Adv" class="c0000FF"><strong>kopā</strong> ar kaķiem</WORD> dzied serenādes vai <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD>  <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c088A08">pa jumta <strong>kori</strong></WORD> , izstiepis rokas</TEXT>
			<COMMENT>Nom V3 paAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>atpūtnieku , <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">kas</WORD> nelāpīja slinkumu , bet , <WORD json="['V1','V2','V3','Inf','Part']" attribute="Part" class="verbs">pastaigājoties</WORD>  <WORD json="['GarDat','GarAcc']" attribute="GarAcc" class="c088A08">gar jūras <strong>krastu</strong></WORD> , mēroja vēl</TEXT>
			<COMMENT>Nom(Acc) V3 garAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>laukā spiedās mūzikas viļņi , <WORD json="['Adv']" attribute="Adv" class="cFF00FF">visur</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD>  <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">medicīnas <strong>studenti</strong></WORD> grupās , pārīšos</TEXT>
			<COMMENT>Adv V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>karstajās smiltīs , spēlē volejbolu , <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD> <WORD json="['GarDat','GarAcc']" attribute="GarAcc" class="c088A08">gar jūras <strong>krastu</strong></WORD> un peldas austrumvēja</TEXT>
			<COMMENT>V3 garAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>nenorietā ar dzergzdi knābī <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">krauklis</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Adv" class="c00FF80"><strong>cienīgi</strong> kā lords</WORD> un ģeķis tas kas Nāvi</TEXT>
			<COMMENT>Nom V3</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>korpuss , otrā — mežs . Bet <WORD json="['BlakusDat','','Loc']" attribute="Loc" class="c00FF00">blakus <strong>placī</strong></WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">sievieši</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Loc" class="c00FF80">sanitāres <strong>uzraudzībā</strong></WORD> . Mūs</TEXT>
			<COMMENT>blakus+Loc V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>dzeltenām mājiņām , kur <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Loc" class="c00FF00">egļu <strong>dārzos</strong></WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD>  <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">tītari</WORD> , kuriem sarkani kakli , kuri salst</TEXT>
			<COMMENT>Loc V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>mēnessērdzīgo , ja domājat , ka <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">viņš</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD>  <WORD json="['PieDat','PieGen']" attribute="PieGen" class="c00FF80">pie pilnas <strong>apziņas</strong></WORD> , un jūs sapratīsiet</TEXT>
			<COMMENT>Nom V3</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['Loc','Nom','Gen','Dat','Acc']" attribute="Loc" class="c00FF00">parkā</WORD> <WORD json="['ArDat','ArAcc']" attribute="ArAcc" class="c0000FF">ar savu nesen dzimušo dēlu <strong>Dāvidu</strong></WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">Tatjana</WORD>. Viņa kopā ar vīru jau</TEXT>
			<COMMENT>Loc V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>[[0 <WORD json="['Loc','Nom','Gen','Dat','Acc']" attribute="Loc" class="cFF00FF">Deli un citās <strong>pilsētās</strong></WORD> <WORD json="['PaDat','PaAcc']" attribute="PaDat" class="c088A08">pa ielām</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">govis</WORD> , kuras īpaši nepieskata pēc tam</TEXT>
			<COMMENT>Loc paDat V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>Ārpus tās , kur <WORD json="['Loc','Nom','Gen','Dat','Acc']" attribute="Loc" class="c00FF00">zālītē</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">baloži , pīles un zvirbuļi</WORD> , mazajiem</TEXT>
			<COMMENT>Loc V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>darāt kaut ko ļoti ierastu, piemēram, <WORD json="['V2','V1','V3','Inf']" attribute="V3" class="verbs">pastaigājaties</WORD> <WORD json="['ArDat','ArAcc']" attribute="ArDat" class="c0000FF">ar suni</WORD> vai gatavojat vakariņas . Šādā</TEXT>
			<COMMENT>V3</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>cik ļoti lepojos ar viņu , kad kopā <WORD json="['V1','V2','V3','Inf']" attribute="V1" class="verbs">pastaigājāmies</WORD> <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c00FF00">pa Mellužu <strong>jūrmalu</strong></WORD> . Viņš , iededzis</TEXT>
			<COMMENT>V1 paAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">Mēs</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Adv" class="c00FF80">lēnām</WORD> <WORD json="['V1','V2','V3','Inf']" attribute="V1" class="verbs">pastaigājāmies</WORD> un nejauši nonācām pie lauku baznīcas</TEXT>
			<COMMENT>Nom V1</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>dendrārisku koku zari , <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c00FF00">pa zālienu</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">Laimas labi aprūpētie <strong>buldogi</strong></WORD> , bet</TEXT>
			<COMMENT>paAcc V3 Nom</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>Nu jau pieaudzis <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">viņš</WORD> <WORD json="['Adv','']" attribute="Adv" class="c00FF80">labprāt</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājas</WORD> <WORD json="['PaDat','PaAcc']" attribute="PaDat" class="c00FF00">pa nepazīstamām <strong>vietām</strong> dabā</WORD></TEXT>
			<COMMENT>Nom V3 paAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT><WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">Tereško</WORD> bauda klusumu un mieru , <WORD json="['V1','V2','V3','Inf','Part']" attribute="Part" class="verbs">pastaigājoties</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Loc" class="c00FF00">Amatas krasta <strong>pļavās</strong></WORD> , dodas uz Gaujas</TEXT>
			<COMMENT>Nom V3 Loc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>limonādi un noraudzījās uz tiem , <WORD json="['Nom','Gen','Dat','Acc','Loc']" attribute="Nom" class="cFFFF00">kas</WORD> <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Adv" class="c00FF80">laiski</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">pastaigājās</WORD> <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c088A08">pa promenādes <strong>ielu</strong></WORD>.</TEXT>
			<COMMENT>Nom V3 paAcc</COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>Visu nakti pavadīšu , <WORD json="['V1','V2','V3','Inf','Part']" attribute="Part" class="verbs">pastaigājoties</WORD> <WORD json="['PaDat','PaAcc']" attribute="PaAcc" class="c00FF00">pa Ziemeļkurzemes <strong>mežiem</strong></WORD> .</TEXT>
			<COMMENT>V1 paAcc</COMMENT>
		</SENTENCE>
	</SENTENCES>
