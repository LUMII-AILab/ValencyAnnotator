		<CATEGORY id="cC0C000">Laiks?</CATEGORY>
		<CATEGORY id="c0000FF">Transporta līdzeklis?</CATEGORY>
		<CATEGORY id="c099C79">Veids?</CATEGORY>
		<CATEGORY id="c8C1476">Virziens</CATEGORY>
		<CATEGORY id="cFF00FF">Laiks: ilgums?</CATEGORY>
