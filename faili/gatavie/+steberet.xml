			<COMMENT></COMMENT>
		</SENTENCE>
		<SENTENCE CATEGORY="blank">
			<TEXT>Ja lielais vēl otrajā dārziņa gadā slimoja, tad <WORD json="['Nom','Gen','Dat','Acc','Loc','V1','V2','V3','Inf','S','TR','Adv']" attribute="Nom" class="cFFFF00">mazā</WORD> <WORD json="['V3','V1','V2','Inf']" attribute="V3" class="verbs">steberēja</WORD> tik <WORD json="['UzDat','UzAcc','UzGen']" attribute="UzAcc" class="c00FFFF">uz dārziņu</WORD>.</TEXT>
